{
    "name": "jeroen-g/laravel-photo-gallery",
    "description": "A photo gallery for Laravel 5",
    "license": "EUPL-1.1",
    "authors": [
        {
            "name": "Jeroen G",
            "email": "jeroeng.jeroeng@gmail.com"
        }
    ],
    "homepage": "https://github.com/Jeroen-G/laravel-photo-gallery",
    "keywords": ["Laravel", "Photo", "Gallery"],

    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.2.*",
        "anahkiasen/flickering": "*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },

    "autoload": {
        "classmap": [
            "src/migrations",
            "src/controllers"
        ],
        "psr-0": {
            "JeroenG\\LaravelPhotoGallery": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JeroenG\\LaravelPhotoGallery\\Tests\\": "tests"
        }
    },
    
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}